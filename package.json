{
  "name": "gpu-sort",
  "version": "0.0.1",
  "description": "gpu accelerated stable sort",
  "main": "lib/index.js",
  "scripts": {
    "bundle-node": "webpack --silent --progress --config-name lib",
    "bundle-browser": "webpack --silent --progress --config-name dist",
    "bench": "npm run bench-node",
    "bench-node": "webpack --silent --progress --config-name bench && node bench/bench.bundle.js",
    "prebench-browser": "webpack --silent --progress --config-name bench-generate && npm --prefix serve-benchmark install serve-benchmark",
    "bench-browser": "npm run prebench-browser && npm run debug --prefix serve-benchmark",
    "test": "npm run test-node",
    "test-node": "webpack --silent --progress --config-name test && node test/test.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/demskie/gpu-sort.git"
  },
  "files": [
    "lib/**/*",
    "bench/**/*",
    "test/**/*"
  ],
  "keywords": [
    "sort",
    "radix",
    "gpu",
    "gpgpu",
    "compute",
    "webgl",
    "glsl",
    "shader"
  ],
  "author": "Alex Demskie",
  "license": "MIT",
  "homepage": "https://github.com/demskie/gpu-sort#readme",
  "bugs": {
    "url": "https://github.com/demskie/gpu-sort/issues"
  },
  "typings": "lib/index.d.ts",
  "browserslist": [
    "android 67",
    "chrome 33",
    "edge 12",
    "firefox 24",
    "ie 11",
    "ios 8",
    "opera 19",
    "safari 8",
    "samsung 4",
    "node 6.5"
  ],
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime-corejs3": "^7.6.0",
    "@types/benchmark": "^1.0.31",
    "@types/node": "^12.7.2",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-loader": "^8.0.4",
    "benchmark": "^2.1.4",
    "core-js": "^3.2.1",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^1.18.2",
    "raw-loader": "^3.1.0",
    "ts-loader": "^6.1.0",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "gpu-compute": "0.0.37"
  }
}
