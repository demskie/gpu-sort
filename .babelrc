{
  "presets": [
    [
      "@babel/preset-env",
      {
        "spec": false,
        "loose": false,
        "useBuiltIns": "entry", // or usage
        "corejs": 3,
        "forceAllTransforms": false
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": 3,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
